{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": [
            "./tsconfig.json"
        ]
    },
    "rules": {
        "prettier/prettier": [ 
            "error", 
            { 
                "endOfLine": "auto" 
            } 
        ]
    }
}